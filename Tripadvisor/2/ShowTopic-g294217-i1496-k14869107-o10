<script type="text/javascript">
var postIndex = {
"121625438":16,"121619645":3,"121624061":14,"121624987":15,"121619802":4,"121623572":10,"121619186":1,"121619504":2,"121623983":13,"124372409":17,"121619020":0,"121623499":9,"121623593":11,"121619816":5,"121621928":7,"121620485":6,"121623748":12,"121622243":8
};
var numReplies = parseInt("17");
var postSortOrder = "oldFirst";
if (window.location.hash && window.location.hash.length > 1) {
postId = window.location.hash.substring(1);
var thisPostIndex = parseInt(postIndex[postId]);
if (!isNaN(thisPostIndex) && thisPostIndex !=0 )
{
if ( postSortOrder == "newFirst")
{
correctOffset = parseInt((numReplies - thisPostIndex) / 10) * 10;
}
else
{
correctOffset = parseInt((thisPostIndex-1) / 10) * 10;
}
if (correctOffset != 10)
{
window.location = require('ta/util/Doer').tadoless('ShowTopic', {g: '294217', i: '1496', k: '14869107', o: '99'}, ['Help_Tips_for_my_16_full_days_in_Hong_Kong', 'Hong_Kong'],'').replace("-o99-","-o"+correctOffset+"-") + "#"+postId;
}
}
}
</script>
